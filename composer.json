{
  "name": "plaisio/menu-core",
  "description": "PhpPlaisio: Core Menu",
  "keywords": [
    "PhpPlaisio",
    "Menu",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/authority": "^1.4.0",
    "plaisio/cache": "^1.2.0",
    "plaisio/cgi": "^1.4.0",
    "plaisio/company-resolver": "^2.2.0",
    "plaisio/core": "^0.30.3",
    "plaisio/db-company": "^1.3.0",
    "plaisio/db-page": "^0.9.3",
    "plaisio/db-profile": "^1.3.0",
    "plaisio/helper-html": "^4.2.0",
    "plaisio/kernel": "^3.3.0",
    "plaisio/menu": "^1.2.0",
    "plaisio/profile": "^0.1.1",
    "plaisio/session": "^5.1.0",
    "plaisio/web-assets": "^3.2.0",
    "setbased/php-stratum-mysql": "^7.1.1"
  },
  "require-dev": {
    "ext-mysqli": "*",
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/authority-core": "^2.3.0",
    "plaisio/babel-core": "^2.2.0",
    "plaisio/cgi-core": "^1.9.0",
    "plaisio/company-resolver-uni": "^2.2.0",
    "plaisio/console": "^3.2.0",
    "plaisio/console-kernel": "^1.2.0",
    "plaisio/console-type-script": "^1.2.0",
    "plaisio/language-resolver-core": "^1.6.0",
    "plaisio/obfuscator-core": "^3.4.0",
    "setbased/php-stratum": "^6.5.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Menu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Menu\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
