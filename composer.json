{
  "name": "plaisio/menu-core",
  "description": "PhpPlaisio: Core Menu",
  "keywords": [
    "PhpPlaisio",
    "Menu",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/authority": "^1.2",
    "plaisio/cache": "1.0.0",
    "plaisio/cgi": "^1.2",
    "plaisio/company-resolver": "^2.0",
    "plaisio/kernel": "^3.1",
    "plaisio/menu": "^1.0.0",
    "plaisio/profile": "dev-master",
    "plaisio/session": "^3.0",
    "plaisio/web-assets": "^1.2"
  },
  "require-dev": {
    "ext-mysqli": "*",
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "plaisio/authority-core": "^1.3",
    "plaisio/babel-core": "^1.1",
    "plaisio/cgi-core": "^1.4",
    "plaisio/company-resolver-uni": "^2.0",
    "plaisio/console": "^2.2",
    "plaisio/core": "^0.11.17",
    "plaisio/db-company": "^1.1",
    "plaisio/db-profile": "^1.1",
    "plaisio/language-resolver-core": "^1.3",
    "plaisio/obfuscator-core": "^3.0",
    "setbased/php-stratum": "^6.0",
    "setbased/php-stratum-mysql": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Menu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Menu\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
      ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
