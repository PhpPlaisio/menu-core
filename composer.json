{
  "name": "plaisio/menu-core",
  "description": "PhpPlaisio: Core Menu",
  "type": "app",
  "keywords": [
    "PhpPlaisio",
    "Menu",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio/authority": "^1.2.0",
    "plaisio/cache": "1.1.0",
    "plaisio/cgi": "^1.3.0",
    "plaisio/company-resolver": "^2.1.0",
    "plaisio/db-company": "^1.2.0",
    "plaisio/db-page": "^0.9.2",
    "plaisio/db-profile": "^1.2.0",
    "plaisio/helper-html": "^4.1.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/menu": "^1.1.0",
    "plaisio/profile": "dev-master",
    "plaisio/session": "^5.0.0",
    "plaisio/web-assets": "^3.1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "ext-mysqli": "*",
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/authority-core": "^2.1.0",
    "plaisio/babel-core": "^2.0.0",
    "plaisio/cgi-core": "^1.8.0",
    "plaisio/company-resolver-uni": "^2.1.1",
    "plaisio/console-kernel": "^1.0.1",
    "plaisio/core": "^0.29.0",
    "plaisio/language-resolver-core": "^1.4.0",
    "plaisio/obfuscator-core": "^3.2.0",
    "setbased/php-stratum": "^6.3.1",
    "setbased/php-stratum-mysql": "^6.6.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Menu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Menu\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
      ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
