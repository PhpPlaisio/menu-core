{
  "name": "plaisio/menu-core",
  "description": "PhpPlaisio: Core Menu",
  "keywords": [
    "PhpPlaisio",
    "Menu",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "plaisio/authority": "^1.2.0",
    "plaisio/cache": "1.0.0",
    "plaisio/cgi": "^1.2.0",
    "plaisio/company-resolver": "^2.0.0",
    "plaisio/db-company": "^1.1.0",
    "plaisio/db-page": "^0.9.1",
    "plaisio/db-profile": "^1.1.0",
    "plaisio/helper-html": "^4.0.0",
    "plaisio/kernel": "^3.1.1",
    "plaisio/menu": "^1.0.0",
    "plaisio/profile": "dev-master",
    "plaisio/session": "^4.0.0",
    "plaisio/web-assets": "^3.0.0"
  },
  "require-dev": {
    "ext-mysqli": "*",
    "phing/phing": "^2.17.2",
    "phpunit/phpunit": "^9.5.16",
    "plaisio/authority-core": "^2.0.1",
    "plaisio/babel-core": "^1.8.0",
    "plaisio/cgi-core": "^1.7.0",
    "plaisio/company-resolver-uni": "^2.0.0",
    "plaisio/console": "^2.7.1",
    "plaisio/core": "^0.28.0",
    "plaisio/language-resolver-core": "^1.3.0",
    "plaisio/obfuscator-core": "^3.1.0",
    "setbased/php-stratum": "^6.1.0",
    "setbased/php-stratum-mysql": "^6.5.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Menu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Menu\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
      ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
